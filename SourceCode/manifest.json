{
  "manifest_version": 2,

  "name": "JTQuotes Start Page",
  "description": "Quotes with clean UI design for new chrome tab.",
  "version": "1.1",

  "chrome_url_overrides" : {
    "newtab": "JTQuotes.html"
  },
  "options_page": "options.html",
  "permissions": [
    "storage",
    "activeTab",
    "tabs",
    "bookmarks",
    "http://quotesondesign.com/"
  ],
  "icons": { 
  	"16": "img/Icon_16.png",
	"48": "img/Icon_48.png",
	"128": "img/Icon_128.png" 
	}
}